{
  "name": "mayasundar.github.io",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && LC_ALL=C find \"out\" -type f -exec sed -i '' 's/\\/_next/\\.\\/_next/g' {} +",
    "start": "next start",
    "lint": "next lint",
    "deploy": "next build && touch out/.nojekyll && cp CNAME out/ && gh-pages -d out"
  },
  "dependencies": {
    "gh-pages": "^6.3.0",
    "next": "^15.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.0.9",
    "typescript": "^5"
  }
}
